{
    "name": "pestphp/dust",
    "description": "A PHP browser testing solution using Laravel Dusk and Pest",
    "keywords": [
        "php",
        "framework",
        "pest",
        "unit",
        "test",
        "testing",
        "dust"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "pestphp/dust-internals": "^0.2"
    },
    "autoload": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "test:unit": "pest --colors=always"
    }
}
